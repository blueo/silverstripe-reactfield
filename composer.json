{
  "name": "blueo/silverstripe-reactfield",
  "description": "React Field boiler plate for SS4",
  "type": "silverstripe-vendormodule",
  "keywords": [
      "silverstripe",
      "react",
      "field",
      "formfield"
  ],
  "license": "BSD-3-Clause",
  "authors": [{
      "name": "Bernie Hamlin",
      "email": "bernie@silverstripe.com"
  }],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
      "silverstripe/cms": "^4.1",
      "silverstripe/admin": "^4.1",
      "silverstripe/vendor-plugin": "^1.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^5.7"
  },
  "extra": {
      "expose": [
          "client/dist"
      ]
  },
  "autoload": {
      "psr-4": {
          "Blueo\\ReactField\\": "src/",
          "Blueo\\ReactField\\Tests\\": "tests/"
      }
  }
}
